{
    "title": "airquality sensor",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "A representation of airquality sensor data",
    "type": "object",
    "properties": {
      "TVOC": { "type": "string" },
      "CO2": { "type": "string" },
      "AQI": { "type": "string" },
      "machine": { "type": "string" },
      "time": { "type": "string",
                "format": "date-time" 
       },
      "topic": { "type": "string" }
    },
    "required": ["TVOC", "CO2", "AQI", "machine", "time","topic"]
} 